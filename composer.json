{
    "name": "wefabric/message-sender",
    "description": "Package for sending data packets to an API endpoint",
    "license": "MIT",
    "keywords": [
        "wefabric",
        "API",
        "endpoint",
        "data"
    ],
    "autoload": {
        "psr-4": {
            "Wefabric\\MessageSender\\": "src/",
            "Wefabric\\MessageSender\\MessageService31_Rexel\\": "MessageService31_Rexel/src/",
            "Wefabric\\MessageSender\\MessageService31_Solar\\": "MessageService31_Solar/src/"
        }
    },
    "authors": [
        {
            "name": "Kevin Nauta",
            "email": "kevin@wefabric.nl"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/var-dumper": "6.1.x-dev"
    },
    "require": {
        "spatie/data-transfer-object": "^2.8",
        "wsdltophp/packagegenerator": "dev-master",
        "wefabric/array-to-simplexml": "*",
        "wefabric/simplexml-to-array": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wefabric/array-to-simplexml"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wefabric/simplexml-to-array"
        }
    ]
}

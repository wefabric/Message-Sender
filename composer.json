{
    "name": "wefabric/message-sender",
    "description": "Package for sending data packets to an API endpoint",
    "license": "MIT",
    "keywords": [
        "wefabric",
        "API",
        "endpoint",
        "data"
    ],
    "autoload": {
        "psr-4": {
            "Wefabric\\MessageSender\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kevin Nauta",
            "email": "kevin@wefabric.nl"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/var-dumper": "6.1.x-dev",
        "wsdltophp/packagegenerator": "dev-master"
    },
    "require": {
        "spatie/data-transfer-object": "^2.8",
        "wefabric/messageservice31_rexel": "*",
        "wefabric/messageservice31_solar": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./MessageService31_Rexel"
        },
        {
            "type": "path",
            "url": "./MessageService31_Solar"
        }
    ]
}
